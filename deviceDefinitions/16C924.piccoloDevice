controller PIC16C924 {
  processor "mid-range" ;
  romsize 4096 ;
  bank 4 ;
  unusedregister 0xD ;
  unusedregister 0x18 to 0x1D ;
  unusedregister 0x8D ;
  unusedregister 0x8F to 0x91 ;
  unusedregister 0x95 to 0x9E ;
  unusedregister 0x105 ;
  unusedregister 0x109 ;
