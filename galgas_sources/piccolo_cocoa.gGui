gui piccolo_cocoa "cocoa" :
with option "piccolo_options.gOption" ;

nibAndMainClass : "PMPiccoloCocoa" ;
nibAndMainClass : "installCLITools" ;

with lexique "piccolo_lexique.gLexique" {
  blockComment : "#" ;
  fileExtension : "piccolo" ;
  title: "Source" ;
  label $routine$, $identifier$ ;
  label $mark$, $literal_string$ ;
  label $ram$, $identifier$ ;
  label $interrupt$, $identifier$ ;
  label $include$, $literal_string$ ;
  label* $commentMark$ (2, 0) ;
}

end gui ;
