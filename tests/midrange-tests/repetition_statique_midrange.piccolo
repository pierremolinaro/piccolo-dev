#---------------------------------------------------------------*
#                                                               *
#  Exemple de répétition statique pour midrange                 *

#                                                               *
#---------------------------------------------------------------*

midrange repetition_statique_midrange "12F683" :

#---------------------------------------------------------------*

noreturn routine main bank:requires 0 {
#--- Instruction de répétition statique
#    La boucle est déroulée
  do (4)
    movlw 0x07
    nop
  end 
#--- 
  forever
  end
}

#---------------------------------------------------------------*

end
