pic18 computed_goto_sample "18F448" :

#----------------------------------------------------------------------------*

routine routine1 bank:preserved {
  mnop 1
}

#----------------------------------------------------------------------------*

routine routine2 bank:preserved {
  mnop 2
}

#----------------------------------------------------------------------------*

routine routine3 bank:preserved {
  mnop 3
}

