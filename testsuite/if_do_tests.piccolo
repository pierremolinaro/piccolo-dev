# This program is just intended for checking
# IF and DO generated code.
# Trying to execute it does not make sense.
 
program if_do_tests "PIC18F448" :

noreturn routine main bank:requires 0 {
#--- IF, elsif, else tests
  if (C)
    CLRF WREG
  elsif (Z)
    CLRF STATUS
  else
    CLRF FSR0L
  end
#--- Or operator
  if (STATUS == W | DECF FSR0L, W Z)
    CLRF WREG
  end
  if (INCF FSR0L NZ)
    CLRF WREG
  end
  if (DECF FSR0L, W Z)
    mnop 1024
  end
  if (STATUS == W)
    CLRF STATUS
  end
  if (STATUS != W)
    CLRF STATUS
  end
  if (STATUS >= W)
    CLRF STATUS
  end
  if (STATUS < W)
    CLRF STATUS
  end
  if (STATUS <= W)
    CLRF STATUS
  end
  if (STATUS > W)
    CLRF STATUS
    mnop 1023
  end

  if (STATUS Z)
    mnop 2
  end
  if (STATUS NZ)
    mnop 2
  end
  if (STATUS Z)
    CLRF STATUS
    if (STATUS NZ)
      mnop 1024
    end
    CLRF STATUS
  end
  forever
  end
}

end
